<ENTRY_CONDITIONS>
(not IS_NONE(State.User__FavoriteFood)) and (
    EXISTS(database_name="food_gpt", key={
        @val(State.User__FavoriteFood | name)
    })
)

<PROMPTS>
Prompt default [ ] {
   @lookup(
        database_name="food_gpt",
        key={@val(State.User__FavoriteFood | name)},
        column="factoid"
   )
}

<SUBNODES>
Subnode food_open_ended_neural_fallback [ ] {
   - @neural_generation(prefix="")
}

<SET_STATE_AFTER>

Flags.GlobalFlag__LastBotTurnWasOpenEnded = {
   @constant(True)
}


